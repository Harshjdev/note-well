{
    "builds":[
        {
        "src":"index.js",
        "use":"@vercel/node"
        },
        {
            "src":"dist/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/create-account",
            "dest":"index.js"
        },
        {
            "src":"/login",
            "dest":"index.js"
        },
        {
            "src":"/get-user",
            "dest":"index.js"
        },
        {
            "src":"/add-note",
            "dest":"index.js"
        },
        {
            "src":"/edit-note/:noteId",
            "dest":"index.js"
        },
        {
            "src":"/get-all-notes/",
            "dest":"index.js"
        },
        {
            "src":"/delete-note/:noteId",
            "dest":"index.js"
        },
        {
            "src":"/update-note-pinned/:noteId",
            "dest":"index.js"
        },
        {
            "src":"/search-notes/",
            "dest":"index.js"
        },
        {
            "src":"/",
            "dest":"dist/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"dist/$1"
        }
    ]
}